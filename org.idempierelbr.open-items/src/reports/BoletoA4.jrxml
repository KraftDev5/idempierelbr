	<queryString>
		<![CDATA[SELECT
  lbr_boleto.lbr_boleto_id AS lbr_boleto_lbr_boleto_id,
  lbr_boletosd.name AS lbr_boleto_name,
  lbr_boletosd.address1 AS lbr_boleto_address1,
  lbr_boletosd.address2 AS lbr_boleto_address2,
  lbr_boletosd.address3 AS lbr_boleto_address3,
  lbr_boletosd.address4 AS lbr_boleto_address4,
  lbr_boletosd.postal AS lbr_boleto_postal,
  lbr_boletosd.city AS lbr_boleto_city,
  lbr_boletosd.regionname AS lbr_boleto_regionname,
  lbr_boletosd.lbr_cnabinscemptype AS lbr_cpfcnpj,
  CASE WHEN lbr_boletosd.lbr_cnabinscemptype = '2'
    THEN regexp_replace(lbr_boletosd.lbr_cnabinscempno,'(..)(...)(...)(....)(..)','\1.\2.\3/\4-\5')
    ELSE regexp_replace(lbr_boletosd.lbr_cnabinscempno,'(...)(...)(...)(..)','\1.\2.\3-\4')
   END AS lbr_inscemp,
  lbr_boleto.datedoc AS lbr_boleto_lbr_docdate,
  lbr_boleto.grandtotal AS lbr_boleto_grandtotal,
  lbr_boleto.duedate AS lbr_boleto_duedate,
  lbr_boleto.lbr_fmt_numberinbank AS lbr_boleto_documentno,
  lbr_boleto.lbr_collectiontype AS lbr_boleto_lbr_collectiontype,
  lbr_boleto.lbr_accepted  AS lbr_boleto_lbr_accepted,
  lbr_boleto.lbr_numberinorg AS lbr_boleto_lbr_numberinorg,
  c_bpartner.name AS lbr_legalentity,
  regexp_replace(c_bpartner.lbr_cnpj,'(..)(...)(...)(....)(..)','\1.\2.\3/\4-\5') as lbr_cnpj,
  regexp_replace(c_bpartner.lbr_cpf,'(...)(...)(...)(..)','\1.\2.\3-\4') as lbr_cpf,
  c_location.address1,
  c_location.address2,
  c_location.address3,
  c_location.city,
  c_location.postal AS postal,
  c_location.regionname AS orgregion,
  lbr_boleto.lbr_fmt_agencyclient AS lbr_conveniono,
  lbr_boleto.lbr_fmt_billfold AS lbr_carteirano,
  lbr_boleto.lbr_typeableline,
  lbr_boleto.lbr_barcode,
  lbr_boleto.lbr_bankuse,
  substring(ad_ref_list.name,'^[^A-Z]+([A-Z]+)') as lbr_boleto_fmt_doctype,
  lbr_boletodetails.lbr_latepaymentpenaltycode,
  lbr_boletodetails.lbr_latepaymentpenaltydate,
  lbr_boletodetails.lbr_latepaymentpenaltyap,
  lbr_boletodetails.lbr_interestcode,
  lbr_boletodetails.lbr_interestdate,
  lbr_boletodetails.lbr_interestap,
  lbr_boletodetails.lbr_cnabdiscount1code,
  lbr_boletodetails.lbr_cnabdiscount1date,
  lbr_boletodetails.lbr_cnabdiscount1ap,
  lbr_boletodetails.lbr_protestcode,
  lbr_boletodetails.lbr_protestdays,
  lbr_boletodetails.lbr_message5,
  lbr_boletodetails.lbr_message6,
  lbr_boletodetails.lbr_message7,
  lbr_boletodetails.lbr_message8,
  lbr_boletodetails.lbr_message9,
  lbr_notafiscal.documentno AS notafiscal,
  CASE WHEN lbr_boleto.lbr_cnab240currency = '09' THEN 'REAL'
     ELSE lbr_boleto.lbr_cnab240currency END AS lbr_boleto_currency,
  c_invoice.documentno AS c_invoice_documentno,
  c_bank.routingno,
  c_bank.lbr_routingcheckdigit,
  c_bank.name as bankname,
  orglogo."binarydata" AS orglogo_binarydata,
  c_bank."logo_id" AS lbr_bank_logo_id,
  ad_image."binarydata" AS lbr_bank_logo_binarydata,
  sacador.name as sacador_name,
  sacador.lbr_bptypebr as sacador_bptypebr,
  regexp_replace(sacador.lbr_cnpj,'(..)(...)(...)(....)(..)','\1.\2.\3/\4-\5') as sacador_cnpj,
  regexp_replace(sacador.lbr_cpf,'(...)(...)(...)(..)','\1.\2.\3-\4') as sacador_cpf
FROM
  adempiere.lbr_boleto
   LEFT JOIN adempiere.lbr_boletosd ON lbr_boleto.lbr_boleto_id = lbr_boletosd.lbr_boleto_id AND lbr_boletosd.lbr_staticdatatype = 'PAG'
   LEFT JOIN adempiere.lbr_boletodetails ON lbr_boleto.lbr_boleto_id = lbr_boletodetails.lbr_boleto_id
   LEFT JOIN adempiere.lbr_bankaccount_convenio ON lbr_boleto.lbr_bankaccount_convenio_id = lbr_bankaccount_convenio.lbr_bankaccount_convenio_id
   LEFT JOIN adempiere.lbr_bankaccount_carteira ON lbr_boleto.lbr_bankaccount_carteira_id = lbr_bankaccount_carteira.lbr_bankaccount_carteira_id
   LEFT JOIN adempiere.c_invoice ON lbr_boleto.c_invoice_id = c_invoice.c_invoice_id
   LEFT JOIN adempiere.c_bank ON lbr_boleto.c_bank_id = c_bank.c_bank_id
   LEFT JOIN adempiere.c_bpartner ON c_bpartner.AD_OrgBP_ID = lbr_boleto.ad_org_id
   LEFT JOIN adempiere.ad_orginfo ON lbr_boleto.ad_org_id = ad_orginfo.ad_org_id
   LEFT JOIN adempiere.ad_org ON lbr_boleto.ad_org_id = ad_org.ad_org_id
   LEFT JOIN ( SELECT DISTINCT ON ("c_bpartner_id") * FROM adempiere.c_bpartner_location bps ORDER BY c_bpartner_id,isbillto DESC) bpl ON bpl.c_bpartner_id = c_bpartner.c_bparTner_id
   LEFT JOIN adempiere.c_location ON c_location.c_location_id=bpl.c_location_id
   LEFT JOIN adempiere.lbr_notafiscal ON lbr_boleto.c_invoice_id = lbr_notafiscal.c_invoice_id
   LEFT JOIN "ad_image" orglogo ON ad_orginfo."logo_id" = orglogo."ad_image_id"
   LEFT JOIN ad_reference ON ad_reference.name = 'LBR_CollectionType'
   LEFT JOIN ad_ref_list ON ad_ref_list.ad_reference_id = ad_reference.ad_reference_id AND ad_ref_list.value = lbr_boleto.lbr_collectiontype
   LEFT JOIN "ad_image" ad_image ON c_bank."logo_id" = ad_image."ad_image_id"
   LEFT JOIN adempiere.c_bpartner sacador ON sacador.C_BPartner_ID = lbr_boleto.lbr_guarantor_id

WHERE (lbr_boleto.C_Invoice_ID = $P{INVOICE_ID} OR lbr_boleto.LBR_Boleto_ID = $P{RECORD_ID}) AND lbr_boleto.DocStatus = 'CO'
ORDER BY
     lbr_boleto.duedate]]>
	</queryString>
